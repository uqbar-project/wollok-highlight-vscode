{
  "name": "wollok-highlight",
  "displayName": "wollok-highlight",
  "description": "Wollok Highlighter",
  "author": "Uqbar Foundation",
  "license": "LGPL-3.0",
  "publisher": "uqbar",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/uqbar-project/wollok-highlight-vscode.git"
  },
  "categories": [
    "Programming Languages"
  ],
	"keywords": [
    "highlight syntax coloring wollok oop learning"
	],
  "icon": "images/wollokHighlightLogo.png",
  "engines": {
    "vscode": "^1.37.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "wollok",
        "aliases": [
          "Wollok",
          "wollok"
        ],
        "extensions": [
          ".wlk",
          ".wtest",
          ".wpgm",
          ".wlk.xt",
          ".wtest.xt",
          ".wpgm.xt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wollok",
        "scopeName": "source.wollok",
        "path": "./syntaxes/wollok.textmate"
      }
    ]
  },
